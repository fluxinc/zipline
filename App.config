<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	 <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
   <section name="Zipline.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
  </sectionGroup>
 </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
	<log4net>
		<appender name="ZiplineLog" type="log4net.Appender.FileAppender">
			<file type="log4net.Util.PatternString" value="%property{LogName}" />
			<appendToFile value="true" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date %level %message%newline" />
			</layout>
		</appender>
		<appender name="UpdateLog" type="log4net.Appender.FileAppender">
			<lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
			<file type="log4net.Util.PatternString" value="%property{UpdateLogName}" />
			<appendToFile value="true" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date %level %message%newline" />
			</layout>
		</appender>

		<root>
			<level value="ALL" />
			<appender-ref ref="ZiplineLog" />
			<appender-ref ref="UpdateLog" />
		</root>
	</log4net>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="BouncyCastle.Crypto" publicKeyToken="0e99375e54769942" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.9.0.0" newVersion="1.9.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <userSettings>
    <Zipline.Properties.Settings>
      <setting name="returnDirectory" serializeAs="String">
        <value>%tmp%\\ZiplineReturnDirectory\\</value>
      </setting>
      <setting name="ignoreHashLog" serializeAs="String">
        <value>false</value>
      </setting>
      <setting name="enableVoice" serializeAs="String">
        <value>true</value>
      </setting>
      <setting name="virtualUSB" serializeAs="String">
        <value />
      </setting>
      <setting name="virtualRepeat" serializeAs="String">
        <value />
      </setting>
    </Zipline.Properties.Settings>
  </userSettings>
</configuration>
